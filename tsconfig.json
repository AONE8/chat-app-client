{
  "files": [],
  "compilerOptions": {
    "types": ["node"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@api": ["./src/api/index.ts"],
      "@api/*": ["./src/api/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@contents/*": ["./src/contents/*"],
      "@hooks": ["./src/hooks/index.ts"],
      "@hooks/*": ["./src/hooks/*"],
      "@layouts": ["./src/layouts/index.tsx"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages": ["./src/pages/index.tsx"],
      "@pages/*": ["./src/pages/*"],
      "@routes": ["./src/routes/index"],
      "@routes/*": ["./src/routes/*"],
      "@schemas/*": ["./src/schemas/*"],
      "@store": ["./src/store/index.ts"],
      "@store/*": ["./src/store/*"],
      "@types/*": ["./src/types/*"],
      "@lib/*": ["./src/lib/*"]
    }
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
